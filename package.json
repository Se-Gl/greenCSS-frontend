{
  "name": "frontend-omen",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "prettier": "prettier ./**/*.{js,jsx,ts,tsx,css,scss,md,json} --config ./.prettierrc.json --write",
    "success-message": "echo \n\n\n🚀  Everything is awesome. 🚀\n\n🖥️  Auto checking completed, ready to deploy.🖥️\n\n☁️  Run: git push ☁️",
    "pre-commit": "npm run prettier && npm run lint --fix && git add -A . && npm run success-message"
  },
  "dependencies": {
    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.43.5"
  },
  "devDependencies": {
    "eslint": "7",
    "eslint-config-next": "12.0.4",
    "husky": "^7.0.0",
    "prettier": "2.5.0"
  }
}
